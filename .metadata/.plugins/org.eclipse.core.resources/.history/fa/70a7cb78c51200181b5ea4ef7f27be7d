package Lesson3;

import java.util.HashSet;
//import java.util.HashSet;
import java.util.LinkedHashSet;

public class Application {

	public static void main(String[] args) {
		LinkedHashSet<Integer> values = new LinkedHashSet<>(); //LinkedHashSet with preserver the order of data inserted while the HashSet wont.
		
		values.add(12);
		values.add(43);
		values.add(15);
		values.add(67);
		values.add(43);
		values.add(33);

		for (Integer value : values) {
			System.out.println(value);
		}
		
		HashSet<Animal> animals = new HashSet<>();
		
		Animal animal = new Animal("Dog", 12);
		Animal animal = new Animal("Cat", 8);
		Animal animal = new Animal("Bird", 3);
		Animal animal = new Animal("Dog", 12);
		Animal animal = new Animal("Kangaroo", 24);
	}

}
